{
  "name": "nestjs-solana-kit",
  "version": "0.1.0",
  "description": "A comprehensive NestJS library for Solana blockchain integration using @solana/kit v5.x",
  "author": "haihv",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "nestjs",
    "solana",
    "blockchain",
    "web3",
    "rpc",
    "crypto",
    "solana-kit",
    "solana-rpc",
    "websocket",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/haihv/nestjs-solana-kit"
  },
  "bugs": {
    "url": "https://github.com/haihv/nestjs-solana-kit/issues"
  },
  "homepage": "https://github.com/haihv/nestjs-solana-kit#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "prepublishOnly": "npm run type-check && npm run build && npm run test"
  },
  "dependencies": {
    "@solana/compat": "^5.0.0",
    "@solana/kit": "^5.0.0",
    "@solana/web3.js": "^1.98.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "reflect-metadata": {
      "optional": false
    },
    "rxjs": {
      "optional": false
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/testing": "^11.1.8",
    "@swc/core": "^1.10.4",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.1.4"
  }
}
